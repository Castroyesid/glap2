                mannerFeatures: 0,
                placeFeatures: 0,
                mappings: {}
            }
        ];

        // Initialize map
        let map = L.map('map').setView([20, 0], 2);
        
        // Add tile layer
        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: '© OpenStreetMap contributors'
        }).addTo(map);

        let markers = [];
        let selectedLanguage = null;

        // Color scheme - gradient from blue (low) to purple (middle) to red (high)
        function getSegmentColor(segments) {
            if (segments <= 4) return '#2E86C1';        // Deep blue for very low
            if (segments >= 15) return '#C0392B';       // Deep red for very high
            
            // Create gradient: blue (4) -> purple (10) -> red (15)
            if (segments <= 10) {
                // Blue to purple: 4-10 segments
                const ratio = (segments - 4) / (10 - 4);
                const r = Math.round(46 + ratio * (155 - 46));   // 46 to 155
                const g = Math.round(134 + ratio * (89 - 134));  // 134 to 89  
                const b = Math.round(193 + ratio * (157 - 193)); // 193 to 157
                return `rgb(${r}, ${g}, ${b})`;
            } else {
                // Purple to red: 10-15 segments  
                const ratio = (segments - 10) / (15 - 10);
                const r = Math.round(155 + ratio * (192 - 155)); // 155 to 192
                const g = Math.round(89 + ratio * (57 - 89));    // 89 to 57
                const b = Math.round(157 + ratio * (43 - 157));  // 157 to 43
                return `rgb(${r}, ${g}, ${b})`;
            }
        }

        function createMarker(language) {
            const color = getSegmentColor(language.elementarySegments);
            
            // Dynamic radius based on zoom level - increased size
            const currentZoom = map.getZoom();
            const radius = Math.max(5, Math.min(30, (currentZoom - 1) * 2.5));
            
            const marker = L.circleMarker([language.lat, language.lng], {
                radius: radius,
                fillColor: color,
                color: '#fff',
                weight: 2,
                opacity: 1,
                fillOpacity: 0.8
            });

            marker.bindPopup(`
                <div>
                    <h3>${language.name}</h3>
                    <p><strong>Elementary Segments:</strong> ${language.elementarySegments}</p>
                    <p><strong>Family:</strong> ${language.family}</p>
                    <button onclick="showLanguageModal(languageData.find(l => l.name === '${language.name}'))" 
                            style="background-color: #3498db; color: white; border: none; padding: 8px 12px; border-radius: 4px; cursor: pointer; margin-top: 8px;">
                        Show Details
                    </button>
                </div>
            `, {
                closeButton: false,
                closeOnClick: true,
                closeOnEscapeKey: true
            });

            marker.on('click', function() {
                selectLanguage(language);
            });

            return marker;
        }

        function updateMarkerSizes() {
            const currentZoom = map.getZoom();
            const radius = Math.max(5, Math.min(30, (currentZoom - 1) * 2.5));
            
            markers.forEach(marker => {
                marker.setRadius(radius);
            });
        }

        function selectLanguage(language) {
            selectedLanguage = language;
            updateLanguageList();
        }

        function updateMap() {
            // Clear existing markers
            markers.forEach(marker => map.removeLayer(marker));
            markers = [];

            // Add new markers
            languageData.forEach(language => {
                const marker = createMarker(language);
                marker.addTo(map);
                markers.push(marker);
            });

            // Add zoom event listener for dynamic sizing
            map.off('zoomend', updateMarkerSizes);
            map.on('zoomend', updateMarkerSizes);

            updateStats();
            updateLanguageGrid();
        }

        function updateStats() {
            const total = languageData.length;
            const completed = languageData.filter(l => l.elementarySegments > 0).length;
            const completedSegments = languageData.filter(l => l.elementarySegments > 0).map(l => l.elementarySegments);
            const avg = completed > 0 ? (completedSegments.reduce((a, b) => a + b, 0) / completed).toFixed(1) : 0;
            
            document.getElementById('totalLanguages').textContent = total;
            document.getElementById('avgSegments').textContent = avg;
            document.getElementById('completedAnalyses').textContent = completed;
        }

        function updateLanguageGrid() {
            const gridContent = document.getElementById('languageGridContent');
            
            let gridHTML = '<div style="display: grid; grid-template-columns: repeat(auto-fill, minmax(200px, 1fr)); gap: 10px; margin-top: 15px;">';
            
            languageData.forEach((language, index) => {
                const segments = language.elementarySegments || 0;
                const color = getSegmentColor(segments);
                const isComplete = segments > 0;
                
                gridHTML += `
                    <div onclick="selectLanguageByIndex(${index})" 
                         style="padding: 10px; border-radius: 4px; cursor: pointer; border: 2px solid ${color}; 
                                background-color: ${isComplete ? 'white' : '#f8f9fa'}; 
                                transition: background-color 0.2s;">
                        <div style="font-weight: bold; margin-bottom: 5px;">${language.name}</div>
                        <div style="font-size: 12px; color: #666;">
                            ${language.family} | ${language.region}
                        </div>
                        <div style="font-size: 14px; color: ${color}; font-weight: bold; margin-top: 5px;">
                            ${isComplete ? `${segments} segments` : 'Pending analysis'}
                        </div>
                    </div>
                `;
            });
            
            gridHTML += '</div>';
            gridContent.innerHTML = gridHTML;
        }

        function selectLanguageByIndex(index) {
            if (index >= 0 && index < languageData.length) {
                selectLanguage(languageData[index]);
                showLanguageModal(languageData[index]);
            }
        }

        // Modal functionality with universal features
        function showLanguageModal(language) {
            const modal = document.getElementById('languageModal');
            const modalName = document.getElementById('modalLanguageName');
            const modalContent = document.getElementById('modalContent');
            
            modalName.textContent = language.name;
            
            // Universal feature set - now expanded for all languages
            const universalFeatures = ["±high", "±low", "±front", "±occlusive", "±obstruent", "±consonantal", "±oral"];
            
            // Create detailed content
            let content = `
                <div class="stats">
                    <div class="stat-item">
                        <span>Elementary Segments:</span>
                        <span>${language.elementarySegments}</span>
                    </div>
                    <div class="stat-item">
                        <span>Elementary Vowels:</span>
                        <span>${language.vowels}</span>
                    </div>
                    <div class="stat-item">
                        <span>Elementary Consonants:</span>
                        <span>${language.consonants}</span>
                    </div>
                    <div class="stat-item">
                        <span>Surface Phonemes:</span>
                        <span>${(language.sourceInventory.vowels?.length || 0) + (language.sourceInventory.consonants?.length || 0) + (language.sourceInventory.marginalConsonants?.length || 0) + (language.sourceInventory.marginalVowels?.length || 0)}</span>
                    </div>
                    <div class="stat-item">
                        <span>Marginal Phonemes:</span>
                        <span>${(language.sourceInventory.marginalConsonants ? language.sourceInventory.marginalConsonants.length : 0) + (language.sourceInventory.marginalVowels ? language.sourceInventory.marginalVowels.length : 0)}</span>
                    </div>
                    <div class="stat-item">
                        <span>Suprasegmentals:</span>
                        <span>${language.sourceInventory.suprasegmentals ? language.sourceInventory.suprasegmentals.length : 0}</span>
                    </div>
                    <div class="stat-item">
                        <span>Total Features:</span>
                        <span>${language.totalFeatures || (language.features ? language.features.list.length : 'TBD')}</span>
                    </div>
                    <div class="stat-item">
                        <span>Manner Features:</span>
                        <span>${language.mannerFeatures || (language.features ? language.features.manner.length : 'TBD')}</span>
                    </div>
                    <div class="stat-item">
                        <span>Place Features:</span>
                        <span>${language.placeFeatures || (language.features ? language.features.place.length : 'TBD')}</span>
                    </div>
                </div>
                
                <div class="stat-item">
                    <span>Surface Phoneme Inventory Source(s):</span>
                    <span>${language.source || 'TBD'}</span>
                </div>
                
                <h3>Elementary Feature Inventory</h3>
                <div class="phoneme-grid">`;
            
            // Show all universal features with status
            universalFeatures.forEach(feature => {
                const isUsed = language.features && language.features.list.includes(feature);
                const featureName = feature.replace('±', '');
                if (isUsed) {
                    content += `<div class="phoneme-item feature-item" data-feature="${featureName}" onclick="toggleFeature('${featureName}')">${feature}</div>`;
                } else {
                    content += `<div class="phoneme-item feature-item inactive">${feature}</div>`;
                }
            });
            
            content += `</div>
                
                <h3>Elementary Segment Inventory</h3>
                <div class="phoneme-grid">`;
            
            language.elementaryInventory.forEach(segment => {
                content += `<div class="phoneme-item elementary-item" data-segment="${segment}">${segment}</div>`;
            });
            
            content += `</div>
                <h3>Source Phoneme Inventory</h3>
                <h4>Surface Vowel Phonemes</h4>
                <div class="phoneme-grid">`;
            
            language.sourceInventory.vowels.forEach(vowel => {
                content += `<div class="phoneme-item">${vowel}</div>`;
            });
            
            content += `</div>
                <h4>Surface Consonant Phonemes</h4>
                <div class="phoneme-grid">`;
            
            language.sourceInventory.consonants.forEach(consonant => {
                content += `<div class="phoneme-item">${consonant}</div>`;
            });
            
            content += `</div>`;
            
            if (language.sourceInventory.marginalConsonants && language.sourceInventory.marginalConsonants.length > 0) {
                content += `<h4>Marginal Consonants</h4>
                    <div class="phoneme-grid">`;
                language.sourceInventory.marginalConsonants.forEach(consonant => {
                    content += `<div class="phoneme-item marginal-item">${consonant}</div>`;
                });
                content += `</div>`;
            }
            
            if (language.sourceInventory.marginalVowels && language.sourceInventory.marginalVowels.length > 0) {
                content += `<h4>Marginal Vowels</h4>
                    <div class="phoneme-grid">`;
                language.sourceInventory.marginalVowels.forEach(vowel => {
                    content += `<div class="phoneme-item marginal-item">${vowel}</div>`;
                });
                content += `</div>`;
            }
            
            if (language.sourceInventory.suprasegmentals && language.sourceInventory.suprasegmentals.length > 0) {
                content += `<h4>Suprasegmentals</h4>
                    <div class="phoneme-grid">`;
                language.sourceInventory.suprasegmentals.forEach(feature => {
                    content += `<div class="phoneme-item suprasegmental-item">${feature}</div>`;
                });
                content += `</div>`;
            }
            
            if (language.features && language.features.matrix && Object.keys(language.features.matrix).length > 0) {
                content += `<h3>Feature Matrix</h3>
                    <div style="font-family: monospace; background-color: #f8f9fa; padding: 15px; border-radius: 4px; overflow-x: auto;">
                        <table style="border-collapse: collapse; width: 100%;">
                            <tr>
                                <th style="border: 1px solid #ddd; padding: 8px; text-align: center;"></th>`;
                
                language.features.list.forEach(feature => {
                    content += `<th style="border: 1px solid #ddd; padding: 8px; text-align: center;">${feature}</th>`;
                });
                
                content += `</tr>`;
                
                Object.entries(language.features.matrix).forEach(([segment, features]) => {
                    content += `<tr><td style="border: 1px solid #ddd; padding: 8px; text-align: center; font-weight: bold;">/${segment}/</td>`;
                    language.features.list.forEach(feature => {
                        const featureName = feature.replace('±', '');
                        const value = features[featureName] || '';
                        content += `<td style="border: 1px solid #ddd; padding: 8px; text-align: center;">${value}</td>`;
                    });
                    content += `</tr>`;
                });
                
                content += `</table></div>`;
            }
            
            if (Object.keys(language.mappings).length > 0) {
                content += `<h3>Mappings</h3>
                    <div style="display: flex; flex-wrap: wrap; gap: 8px; margin: 15px 0; align-items: flex-start;">`;
                
                Object.entries(language.mappings).forEach(([surface, elementary]) => {
                    content += `<span style="padding: 6px 10px; background-color: #f8f9fa; border-radius: 4px; font-family: monospace; font-size: 14px; white-space: nowrap;">${surface} → ${elementary}</span>`;
                });
                
                content += `</div>`;
            }
            
            content += `<p><strong>Notes:</strong> ${language.notes}</p>`;
            
            modalContent.innerHTML = content;
            
            // Feature interaction state
            let activeFeature = null;
            let featureState = 'neutral'; // 'neutral', 'positive', 'negative'
            
            // Feature interaction function
            window.toggleFeature = function(featureName) {
                // Reset all highlighting
                document.querySelectorAll('.phoneme-item').forEach(item => {
                    item.classList.remove('highlighted', 'positive', 'negative');
                });
                
                document.querySelectorAll('.feature-item').forEach(item => {
                    item.classList.remove('active', 'positive', 'negative');
                    // Reset text to original
                    const originalFeature = item.getAttribute('data-feature');
                    if (originalFeature) {
                        item.textContent = `±${originalFeature}`;
                    }
                });
                
                // Cycle through states
                if (activeFeature === featureName) {
                    if (featureState === 'neutral') {
                        featureState = 'positive';
                    } else if (featureState === 'positive') {
                        featureState = 'negative';
                    } else {
                        featureState = 'neutral';
                        activeFeature = null;
                        return;
                    }
                } else {
                    activeFeature = featureName;
                    featureState = 'positive';
                }
                
                // Update feature display
                const featureElement = document.querySelector(`[data-feature="${featureName}"]`);
                if (featureElement) {
                    featureElement.classList.add('active');
                    if (featureState === 'positive') {
                        featureElement.classList.add('positive');
                        featureElement.textContent = `+${featureName}`;
                    } else if (featureState === 'negative') {
                        featureElement.classList.add('negative');
                        featureElement.textContent = `-${featureName}`;
                    }
                }
                
                // Highlight matching segments
                if (language.features && language.features.matrix) {
                    Object.entries(language.features.matrix).forEach(([segment, features]) => {
                        const value = features[featureName];
                        let shouldHighlight = false;
                        
                        if (featureState === 'positive' && (value === '+' || value === '±')) {
                            shouldHighlight = true;
                        } else if (featureState === 'negative' && (value === '-' || value === '±')) {
                            shouldHighlight = true;
                        }
                        
                        if (shouldHighlight) {
                            const segmentElement = document.querySelector(`[data-segment="${segment}"]`);
                            if (segmentElement) {
                                segmentElement.classList.add('highlighted');
                                if (featureState === 'positive') {
                                    segmentElement.classList.add('positive');
                                } else if (featureState === 'negative') {
                                    segmentElement.classList.add('negative');
                                }
                            }
                        }
                    });
                }
            };
            
            modal.style.display = 'block';
        }

        function closeModal() {
            document.getElementById('languageModal').style.display = 'none';
        }

        // Event listeners for modal
        document.addEventListener('DOMContentLoaded', function() {
            // Close button
            document.querySelector('.close').addEventListener('click', closeModal);
            
            // Click outside modal to close
            document.getElementById('languageModal').addEventListener('click', function(e) {
                if (e.target === this) {
                    closeModal();
                }
            });
            
            // Escape key to close
            document.addEventListener('keydown', function(e) {
                if (e.key === 'Escape') {
                    closeModal();
                }
            });
        });

        // Initialize
        updateMap();
    </script>
</body>
</html>
